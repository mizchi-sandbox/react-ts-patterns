{
  "compilerOptions": {
    "noEmit": true,
    "baseUrl": "./src/",
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "esnext",
    "jsx": "react",
    "allowJs": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "./src/**/*.test.ts", "./src/**/*.test.tsx"]
}
